        <parameter name="rlt" value="1e+09" dev="100000000"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="1" dev="0.5"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.60000002"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" number="0" type="2" percentage="1"/>
