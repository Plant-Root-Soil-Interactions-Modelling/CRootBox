<plant>
    <seed name="maize_p2_zero_std" subType="0">
        <parameter name="delayB" value="7" dev="0"/>
        <!--Time delay between the basal roots [day]-->
        <parameter name="delayRC" value="1000" dev="0"/>
        <!--Delay between the root crowns [day]-->
        <parameter name="delaySB" value="1000" dev="0"/>
        <!--Time delay between the shoot borne roots [day]-->
        <parameter name="firstB" value="7" dev="0"/>
        <!--Emergence of first basal root [day]-->
        <parameter name="firstSB" value="1000" dev="0"/>
        <!--First emergence of a shoot borne root [day]-->
        <parameter name="maxB" value="8" dev="0"/>
        <!--Maximal number of basal roots [1]-->
        <parameter name="nC" value="0" dev="0"/>
        <!--Maximal number of roots per root crown [1]-->
        <parameter name="nz" value="0" dev="0"/>
        <!--Distance between the root crowns along the shoot [cm]-->
        <parameter name="seedPos.x" value="0" dev="0"/>
        <!--X-Coordinate of seed position [cm]-->
        <parameter name="seedPos.y" value="0" dev="0"/>
        <!--Y-Coordinate of seed position [cm]-->
        <parameter name="seedPos.z" value="-1.2" dev="0"/>
        <!--Z-Coordinate of seed position [cm]-->
        <parameter name="simtime" value="60" dev="0"/>
        <!--Recommended final simulation time  [day]-->
    </seed>
    <root name="primaryroots" subType="1">
        <parameter name="gf" value="1" dev="100000000"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="1"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="3.9000001" dev="0"/>
        <!--Root radius [cm]-->
        <parameter name="colorB" value="205"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="0"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="0"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="0.5"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="la" value="24.200001" dev="0"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="0.5" dev="0"/>
        <!--Basal zone [cm]-->
        <parameter name="ln" value="0.5" dev="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="nob" value="432" dev="0"/>
        <!--Maximal number of laterals [1]-->
        <parameter name="r" value="3.9000001" dev="0"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09" dev="100000000"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="1.0599999" dev="0.139626"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="1"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.1"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
        <parameter name="successor" number="0" type="2" percentage="1"/>
        <!--Sub type of lateral roots-->
    </root>
    <root name="secondaryroots" subType="2">
        <parameter name="gf" value="1" dev="100000000"/>
        <!--Growth function number [1]-->
        <parameter name="tropismT" value="2"/>
        <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
        <parameter name="a" value="0.75" dev="0"/>
        <!--Root radius [cm]-->
        <parameter name="colorB" value="0"/>
        <!--Root color, blue component [0.-1.]-->
        <parameter name="colorG" value="23.799999"/>
        <!--Root color, green component [0.-1.]-->
        <parameter name="colorR" value="238"/>
        <!--Root color, red component [0.-1.]-->
        <parameter name="dx" value="0.5"/>
        <!--Axial resolution [cm] (maximal segment size)-->
        <parameter name="la" value="10" dev="0"/>
        <!--Apical zone [cm]-->
        <parameter name="lb" value="0" dev="0"/>
        <!--Basal zone [cm]-->
        <parameter name="ln" value="1.2" dev="0"/>
        <!--Inter-lateral distance [cm]-->
        <parameter name="nob" value="5.5511151e-16" dev="0"/>
        <!--Maximal number of laterals [1]-->
        <parameter name="r" value="0.75" dev="0"/>
        <!--Initial growth rate [cm day-1]-->
        <parameter name="rlt" value="1e+09" dev="100000000"/>
        <!--Root life time [day]-->
        <parameter name="theta" value="1.50098" dev="0.15009800000000001"/>
        <!--Angle between root and parent root [rad]-->
        <parameter name="tropismN" value="0.5"/>
        <!--Number of trials of root tropism-->
        <parameter name="tropismS" value="0.1"/>
        <!--Mean value of expected change of root tropism [1/cm]-->
    </root>
</plant>
